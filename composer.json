{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "ashallendesign/laravel-exchange-rates": "^4.1",
        "backpack/crud": "4.1.*",
        "backpack/logmanager": "^4.0",
        "backpack/newscrud": "^4.0",
        "backpack/permissionmanager": "^6.0",
        "backpack/settings": "^3.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "bavix/laravel-wallet": "^6.0",
        "dingo/api": "^3.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "7.2",
        "intervention/image": "^2.5",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^7.0",
        "laravel/telescope": "3.5.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.4",
        "laravelista/comments": "3.6.1",
        "mcamara/laravel-localization": "^1.6",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "munafio/chatify": "^1.2",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-translatable": "^4.6",
        "staudenmeir/belongs-to-through": "^2.5",
        "stevebauman/location": "^6.1",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "backpack/generators": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laracasts/generators": "^2.0",
        "mockery/mockery": "^1.3.1",
        "mpociot/laravel-test-factory-helper": "^2.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "wnx/laravel-stats": "^2.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Functions/admin.php",
            "app/Helpers/Functions/core.php",
            "app/Helpers/Functions/plugin.php",
            "app/Helpers/Functions/wordpress.php",
            "app/Helpers/Functions/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models"
        ],
        "clear-app": [
            "@php artisan config:cache",
            "@php artisan optimize",
            "@php artisan optimize:clear",
            "@php artisan route:cache",
            "@php artisan route:clear",
            "@php artisan clear-compiled"
        ]
    }
}
